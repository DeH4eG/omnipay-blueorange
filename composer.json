  "keywords": [
    "omnipay",
    "blueorange",
    "blueorangebank",
    "blue orange",
    "payment",
    "gateway",
    "acquiring"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Denis Smolakov",
      "email": "denissmolakovs@gmail.com"
    }
  ],
  "autoload": {
